<!doctype html>
<html>
  <head>
    <title>About <%=shopName%></title>
    <link rel="stylesheet"  type="text/css" href="main.css" />
  </head>
  <html >
    <head>
      <title>Document</title>
    </head>
    <body bgcolor="PeachPuff">
      <nav>
        <ul>
          <li><a href='./'>Home</a></li>
          <li><a href="about">About</a></li>
          <li><a href="register">Register</a></li>
          <li><a href="login">Log In</a></li>
          <li><a href="addFood">Add Food</a></li>
          <li><a href="searchFood">Search Food</a></li>
          <li><a href="updateFood-Search">Update Food</a></li>
          <li><a href="deleteFood">Delete Food</a></li>
          <li><a href="foodList">Food List</a></li>
          <li><a href="logout">Log Out</a></li>
        </ul>
      </nav>
      <h1>Record of Food on <%=shopName%> :</h1>
      <table>
        <tr>
            <th>Ingredient</th>
            <th>Values per</th>
            <th>Carbs</th>
            <th>Fats</th>
            <th>Proteins</th>
            <th>Salt</th>
            <th>Sugar</th>
        </tr>
        <% availableIngredients.forEach(function(food) { %>
            <tr>
                <td><%=food.ingred_name%></td>
                <td><%=food.value_per%> <%=food.unit%></td>
                <td><%=food.carbs%></td>
                <td><%=food.fats%></td>
                <td><%=food.protein%></td>
                <td><%=food.salt%></td>
                <td><%=food.sugar%></td>
            </tr>
        <% }) %>
      </table><br>
      <input type="submit" value="Delete Record">
  </body>
</html>
